{
  "name": "cityline",
  "version": "1.0.0",
  "description": "Unit converter",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint ./ && jscs ./",
    "coveralls": "tap --coverage-report=text-lcov | coveralls",
    "start": "node index.js",
    "test": "tap --timeout=200 --cov test/test-*.*",
    "posttest": "tap --coverage-report=lcov --no-browser && tap --coverage-report=text"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/setogit/cityline.git"
  },
  "license": "MIT",
  "author": "Tetsuo Seto <setogit@gmail.com>",
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-strongloop": "^2.1.0",
    "jscs": "^3.0.7",
    "request": "^2.85.0",
    "tap": "^11.1.4"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "mathjs": "^4.2.2",
    "urlencode": "^1.1.0"
  }
}
